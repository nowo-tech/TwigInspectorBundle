{
    "name": "nowo-tech/twig-inspector-demo-symfony8",
    "description": "Demo project for Twig Inspector Bundle - Symfony 8.0",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2,<8.6",
        "nowo-tech/twig-inspector-bundle": "^0.0.1",
        "symfony/console": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/flex": "^2.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/runtime": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/web-profiler-bundle": "^8.0",
        "symfony/asset-mapper": "^8.0",
        "symfony/yaml": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "audit": {
            "block-insecure": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../",
            "options": {
                "symlink": true
            }
        }
    ]
}
